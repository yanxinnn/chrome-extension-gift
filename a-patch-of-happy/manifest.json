{
  "name": "A Patch of Happy",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Replaces the new tab page with a customized space made for you, because you're my happy.",
  "browser_action": {
    "default_icon": "leaf.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "leaf.png",
           "48": "leaf.png",
          "128": "leaf.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "tabs"
  ]
}
